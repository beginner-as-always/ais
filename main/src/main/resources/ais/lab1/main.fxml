<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Button?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      prefHeight="400.0" prefWidth="600.0"
      stylesheets="@css/style.css"
      styleClass="main">

    <HBox styleClass="container">
        <Label text="Обработка знаний" fx:id="title" />
    </HBox>

    <HBox VBox.vgrow="ALWAYS" spacing="10">
        <VBox styleClass="container">
            <CheckBox text="Checkbox1" />
            <CheckBox text="Checkbox2" />
            <CheckBox text="Checkbox3" />
            <CheckBox text="Checkbox4" />
        </VBox>
        <VBox styleClass="container" HBox.hgrow="ALWAYS">
            <Button text="OK" />
        </VBox>
    </HBox>

</VBox>
